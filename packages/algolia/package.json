{
    "name": "@nativescript-community/algolia",
    "version": "2.0.0",
    "description": "NativeScript plugin for Algolia search. This plugin is designed to mirror, as closely as possible, the structure of Algoliaâ€™s JavaScript client.",
    "main": "./algolia",
    "typings": "./algolia.d.ts",
    "scripts": {
        "build": "npm run tsc && npm run readme",
        "readme": "../../node_modules/.bin/readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "../../node_modules/.bin/cpy '**/*.d.ts' '../../packages/algolia' --parents --cwd=../../src/algolia && ../../node_modules/.bin/tsc -skipLibCheck -d",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "algolia",
        "search"
    ],
    "bugs": {
        "url": "https://github.com/nativescript-community/algolia/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nativescript-community/algolia"
    },
    "license": "Apache-2.0",
    "readmeFilename": "README.md"
}
